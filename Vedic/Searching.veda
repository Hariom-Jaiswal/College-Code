#Linear Search

मान अ = [१,२,४,५,८६,४,७५,४,१००];

वद("Enter the Value: ");
मान target = अंक(पठन());
मान flag = 0;

चक्र(मान क = 0; क<कुल(अ); क=क+१){
    यदि(अ[क]==target){
        वद("Target Found yayaya at index: ",क);
        flag = 1;
        विराम;
    }
}

यदि(flag!=1){
    वद("No result");
}


